// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Zones {
  id             Int      @id @default(autoincrement())
  zone           Int      @unique
  description    String
  notableRegions String
}

model Users {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  firstName  String
  lastName   String
  zoneId     Zones    @relation(fields: [zoneId], references: [id])
  zoneId     Int      @unique
}